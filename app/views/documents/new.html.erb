<div class="text-center row">
  <p><%=notice%></p>
  <%=form_with(model: @document, local: true) do |f|%>
    <% if @document.errors.any? %>
      <div id="error_explanation", class="a">
        <h2><%= pluralize(@document.errors.count, "error") %> prohibited this post from being saved:</h2>
        <ul>
          <% @document.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <%=f.label :Name%><br>
    <%=f.text_field :name%><br>
    <%=f.label :File%><br>
    <%=f.file_field :file%><br>
    <%=f.submit :Create%>
  <%end%>
  <%=link_to "Back_to_Users", documents_path%>
</div>
